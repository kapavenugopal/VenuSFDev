  <apex:sectionHeader title="Job Applications" subtitle="Job Application"/ >
  <hr/>
   <chatter:feedwithFollowers entityId="{!Job_Application__c.Id}"/>
<apex:form styleClass="myFormStyle">
  <apex:pageBlock title="Job Application Detail" mode="maindetail">
  <center>
   <apex:commandButton value="Edit" action="{!edit}" rendered="{!(($Profile.Name == "Recruiter")||($Profile.Name == "System Administrator"))}"/>
      <apex:commandButton value="Delete" action="{!delete}"/>
      <apex:commandButton action="{!URLFOR($Action.Job_Application__c.Clone, Id)}" id="Clone" value="Clone" />
  </center> 
  <apex:pageBlockSection columns="2" showHeader="true" >
     <apex:outputField value="{!Job_Application__c.Name}" />
     <apex:outputField value="{!Job_Application__c.Ownerid}"/>
        <apex:outputField value="{!Job_Application__c.Candidate__c}" />
        <apex:outputField value="{!Job_Application__c.CEO_Approved__c}"/>
          </apex:pageBlockSection> 
        <apex:pageBlockSection columns="1" showHeader="true" >
   <apex:outputField value="{!Job_Application__c.Position__c}" />
   <apex:outputField value="{!Job_Application__c.Cover_Letter__c}" rendered="{!NOT(ISBLANK(Job_Application__c.Cover_Letter__c))}" />
     <apex:outputField value="{!Job_Application__c.Status__c}" />
        <apex:outputField value="{!Job_Application__c.Total_Rating__c}" />
           <apex:outputField value="{!Job_Application__c.Number_of_Reviews__c}" />
           <apex:outputField value="{!Job_Application__c.Average_Rating__c}" />
               </apex:pageBlockSection> 
           <apex:pageBlockSection columns="2" showHeader="true" >
            <apex:outputField value="{!Job_Application__c.CreatedByid}"/>
           <apex:outputField value="{!Job_Application__c.LastModifiedByid}"/>
           
    
               </apex:pageBlockSection> 
    </apex:pageBlock>
  </apex:form>

        <apex:repeat value="{!Job_Application__c.OpenActivities}" var="temp" rows="1" >
            <div class="row">
            <apex:relatedList list="OpenActivities" />
            </div>
        </apex:repeat>
        <apex:repeat value="{!Job_Application__c.ActivityHistories}" var="temp" rows="1">
            <div class="row">
            <apex:relatedList list="ActivityHistories"/>
            </div>
        </apex:repeat>
        <apex:repeat value="{!Job_Application__c.Reviews__r}" var="temp" rows="1">
            <div class="row">
            <apex:relatedList list="Reviews__r"/>
            </div>
        </apex:repeat>

</apex:page>