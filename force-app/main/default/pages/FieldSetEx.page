<apex:page standardController="Account">
    <apex:repeat value="{!$ObjectType.Account.FieldSets.AccountFields}" var="f"> 
        <apex:outputText value="{!Account[f]}" /><br/>
    </apex:repeat>

    <apex:pageBlock title="Fields in Proper Names">
        <apex:pageBlockTable value="{!$ObjectType.Account.FieldSets.AccountFields}" var="f"> 
            <apex:column value="{!f.Label}">
                <apex:facet name="header">Label</apex:facet>
            </apex:column> 
            <apex:column value="{!Account[f]}">
                <apex:facet name="header">Name</apex:facet>
            </apex:column>
            <apex:column value="{!f.Type}" >
                <apex:facet name="header">Data Type</apex:facet>
            </apex:column> 
        </apex:pageBlockTable> 
    </apex:pageBlock> 
</apex:page>